@model CV_Web.Models.EntityFramework.TBLEGİTİMLERİM

@{
    ViewBag.Title = "EgitimGetir";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h2>Egitim Güncelleme Sayfası</h2>
<br />


<form class="form-group" method="post">
    <div>
        @Html.Label("ID")
        @Html.TextBoxFor(x => x.ID, new { @class = "form-control" })
    </div>

    <div>
        @Html.Label("Eğitim Seviyesi")
        @Html.TextBoxFor(x => x.Baslik, new { @class = "form-control" })
    </div>

    <div>
        @Html.Label("Okul")
        @Html.TextBoxFor(x => x.AltBaslik1, new { @class = "form-control" })
    </div>

    <div>
        @Html.Label("Bölüm")
        @Html.TextBoxFor(x => x.AltBaslik2, new { @class = "form-control" })
    </div>

    <div>
        @Html.Label("Gano")
        @Html.TextBoxFor(x => x.GNO, new { @class = "form-control" })
    </div>

    <div>
        @Html.Label("Tarih")
        @Html.TextBoxFor(x => x.Tarih, new { @class = "form-control" })
    </div>
    <br />
    <button class="btn btn-success">Güncelle</button>
</form>
